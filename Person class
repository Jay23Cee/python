class person:
	 def __init__(self, name, age, address, zipcode, email, phone):
	 	self.name = name
	 	self.age = age
	 	self.address= address
	 	self.zipcode=zipcode
	 	self.email=email
	 	self.phone= phone
	 	
	 def toString(self):
	 	mystring = self.name + ' '+ str(self.age)
	 	return mystring
	 
	 def set_name(self, name):
	 	self.name = name
	 
	 def set_age(self, age):
	 	self.age = age
	
	 def set_address(self, address):
	 	self.address = address

	 def set_zipcode(self, zipcode):
	 	self.zipcode = zipcode
	 
	 def set_email(self, email):
	 	self.email = email

	 def set_phone(self, phone):
	 	self.phone = phone
	 	
	 	
	 	
	 	
	 	
	 	
	 	
    
    
p1= person("jack", 29, "123 fake st",90016, "justan@example", "(323)123-4321")

print(p1.toString())

print(p1)
